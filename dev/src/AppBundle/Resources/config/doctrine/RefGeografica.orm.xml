<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RefGeografica" table="ref_geografica">
    <indexes>
      <index name="agrupa" columns="agrupa"/>
    </indexes>
    <id name="idRefgeografica" type="integer" column="id_refgeografica">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="5000" nullable="false"/>
    <field name="ambito" type="string" column="ambito" nullable="false"/>
    <field name="agrupa" type="integer" column="agrupa" nullable="false"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Metas" table="metas">
    <indexes>
      <index name="fk_id_objetivo" columns="fk_id_objetivo"/>
    </indexes>
    <id name="idMeta" type="integer" column="id_meta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="5000" nullable="false"/>
    <many-to-one field="fkObjetivo" target-entity="Objetivos">
      <join-columns>
        <join-column name="fk_id_objetivo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

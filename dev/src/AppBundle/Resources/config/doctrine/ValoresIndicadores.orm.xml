<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ValoresIndicadores" table="valores_indicadores">
    <indexes>
      <index name="id_etiqueta" columns="id_etiqueta"/>
      <index name="id_refgeografica" columns="id_refgeografica"/>
      <index name="IDX_53CEDF144D2782E4" columns="id_indicador"/>
    </indexes>
    <id name="idRefgeografica" association-key="true"/>
    <id name="idIndicador" association-key="true"/>
    <id name="idEtiqueta" association-key="true"/>
    <field name="anio" type="date" column="anio" nullable="false"/>
    <field name="mes" type="boolean" column="mes" nullable="false"/>
    <field name="valor" type="decimal" column="valor" precision="10" scale="2" nullable="false"/>
    <field name="aprobado" type="boolean" column="aprobado" nullable="false"/>
    <one-to-one field="idEtiqueta" target-entity="Etiquetas">
      <join-columns>
        <join-column name="id_etiqueta" referenced-column-name="id_etiqueta"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idIndicador" target-entity="Indicadores">
      <join-columns>
        <join-column name="id_indicador" referenced-column-name="id_indicador"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idRefgeografica" target-entity="RefGeografica">
      <join-columns>
        <join-column name="id_refgeografica" referenced-column-name="id_refgeografica"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>

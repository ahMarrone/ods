<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Agrupamientorefgeografica" table="agrupamientoRefGeografica">
    <indexes>
      <index name="id1" columns="id1"/>
      <index name="id2" columns="id2"/>
    </indexes>
    <id name="id1" type="integer" column="id1"/>
    <id name="id2" type="integer" column="id2"/>
  </entity>

  <one-to-one field="id1" target-entity="Refgeografica">
      <join-columns>
        <join-column name="id1" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  <one-to-one field="id2" target-entity="Refgeografica">
      <join-columns>
        <join-column name="id2" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
</doctrine-mapping>
  <one-to-one field="id_1" target-entity="Refgeografica">
      <join-columns>
        <join-column name="id_1" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  <one-to-one field="id_2" target-entity="Refgeografica">
      <join-columns>
        <join-column name="id_2" referenced-column-name="id_2"/>
      </join-columns>
    </one-to-one>
</doctrine-mapping>
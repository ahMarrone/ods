<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Desglocesindicadores
 */
class Desglocesindicadores
{
    /**
     * @var integer
     */
    private $idindicador;

    /**
     * @var integer
     */
    private $iddesgloce;


    /**
     * Set idindicador
     *
     * @param integer $idindicador
     * @return Desglocesindicadores
     */
    public function setIdindicador($idindicador)
    {
        $this->idindicador = $idindicador;

        return $this;
    }

    /**
     * Get idindicador
     *
     * @return integer 
     */
    public function getIdindicador()
    {
        return $this->idindicador;
    }

    /**
     * Set iddesgloce
     *
     * @param integer $iddesgloce
     * @return Desglocesindicadores
     */
    public function setIddesgloce($iddesgloce)
    {
        $this->iddesgloce = $iddesgloce;

        return $this;
    }

    /**
     * Get iddesgloce
     *
     * @return integer 
     */
    public function getIddesgloce()
    {
        return $this->iddesgloce;
    }
}
